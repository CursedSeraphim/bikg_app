@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rut:   <http://rdfunit.aksw.org/ns/core#> .

sh:shape1 a
    sh:PropertyShape;
  sh:path sh:hasDisease;
  sh:class sh:Disease .

sh:violation1
        a                             sh:ValidationResult , rut:TestCaseResult ;
        sh:sourceShape                sh:shape1 ;
        sh:edge1                       sh:object1 ;
        sh:edge2                       sh:object2 ;
        sh:edge3                       sh:object3 ;
        sh:edge4                       sh:object4 .