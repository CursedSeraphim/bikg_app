@prefix ex: <http://example.com/exemplar#> .
@prefix ns1: <http://customnamespace.com/> .
@prefix ont: <http://exampleontology.com/ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rlog: <http://persistence.uni-leipzig.org/nlp2rdf/ontologies/rlog#> .
@prefix rut: <http://rdfunit.aksw.org/ns/core#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ont:DirectorShape a sh:NodeShape ;
    sh:node ont:ManagerShape ;
    sh:targetClass ont:Director .

ex:EmployeeIDShape_exemplar_2 a rut:TestCaseResult,
        sh:PropertyShape,
        sh:ValidationResult ;
    prov:wasGeneratedBy <urn:uuid:b8916a6e-83ab-44d4-97d4-86f6238c13a0> ;
    sh:message "value not of datatype <http://www.w3.org/2001/XMLSchema#string>" ;
    sh:resultMessage "value not of datatype {$datatype}" ;
    sh:resultPath ont:employeeID ;
    sh:resultSeverity sh:Violation ;
    sh:severity rlog:ERROR ;
    sh:sourceConstraintComponent sh:DatatypeConstraintComponent ;
    sh:value 123 .

ex:ManagerTeamSizeShape_exemplar_1 a rut:TestCaseResult,
        sh:PropertyShape,
        sh:ValidationResult ;
    prov:wasGeneratedBy <urn:uuid:b8916a6e-83ab-44d4-97d4-86f6238c13a0> ;
    sh:message "sh:minInclusive of <http://example.com/ns#teamSize> should be \"6\"^^<http://www.w3.org/2001/XMLSchema#integer>" ;
    sh:resultMessage "sh:minInclusive of {$PATH} should be {$minInclusive}" ;
    sh:resultPath ont:teamSize ;
    sh:resultSeverity sh:Violation ;
    sh:severity rlog:ERROR ;
    sh:sourceConstraintComponent sh:MinInclusiveConstraintComponent ;
    sh:value 5 .

ont:Director a owl:Class ;
    rdfs:subClassOf ont:Employee .

ont:EmployeeIDShape a sh:PropertyShape ;
    ns1:hasExemplar ex:EmployeeIDShape_exemplar_2 ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path ont:employeeID .

ont:EmployeeNameShape a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path ont:name .

ont:Manager a owl:Class ;
    rdfs:subClassOf ont:Employee .

ont:ManagerShape a sh:NodeShape ;
    sh:node ont:EmployeeShape ;
    sh:property ont:ManagerShapehasSubordinate,
        ont:ManagerTeamSizeShape ;
    sh:targetClass ont:Manager .

ont:ManagerShapehasSubordinate a sh:PropertyShape ;
    sh:node ont:EmployeeShape ;
    sh:path ont:hasSubordinate .

ont:ManagerTeamSizeShape a sh:PropertyShape ;
    ns1:hasExemplar ex:ManagerTeamSizeShape_exemplar_1 ;
    sh:datatype xsd:integer ;
    sh:minInclusive 6 ;
    sh:path ont:teamSize .

ont:EmployeeShape a sh:NodeShape ;
    sh:property ont:EmployeeIDShape,
        ont:EmployeeNameShape ;
    sh:targetClass ont:Employee .

ont:Employee a owl:Class .

